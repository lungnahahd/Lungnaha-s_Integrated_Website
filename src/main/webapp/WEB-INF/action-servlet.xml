<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="org.springframework.web.servlet.handler.SimpleUrlHandlerMapping">
		<property name="mappings">
			<props>
				<prop key="/login.do">login</prop>
				<prop key="/getBoardList.do">getBoardList</prop>
				<prop key="/insertBoard.do">insertBoard</prop>
				<prop key="/getBoard.do">getBoard</prop>
				<prop key="/updateBoard.do">updateBoard</prop>
				<prop key="/deleteBoard.do">deleteBoard</prop>
			</props>
		</property>
	</bean>
	
	<bean id="login" class="com.Lungnaha.IntegratedWebsite.mvcUser.LoginController"></bean>
	<bean id="getBoardList" class="com.Lungnaha.IntegratedWebsite.mvcBoard.GetBoardListController"></bean>
	<bean id="insertBoard" class="com.Lungnaha.IntegratedWebsite.mvcBoard.InsertBoardController"></bean>
	<bean id="getBoard" class="com.Lungnaha.IntegratedWebsite.mvcBoard.GetBoardController"></bean>
	<bean id="updateBoard" class="com.Lungnaha.IntegratedWebsite.mvcBoard.UpdateBoardController"></bean>
	<bean id="deleteBoard" class="com.Lungnaha.IntegratedWebsite.mvcBoard.DeleteBoardController"></bean>
</beans>
